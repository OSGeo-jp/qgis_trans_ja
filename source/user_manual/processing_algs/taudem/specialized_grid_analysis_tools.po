msgid "Topographic wetness index"
msgstr ""

#: ../../docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:1068
msgid "Calculates the topographic wetness index (TWI)."
msgstr ""

#: ../../docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:1075
msgid "``Slope`` [raster]"
msgstr ""

#: ../../docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:1082
msgid "``Specific catchment area`` [raster]"
msgstr ""

#: ../../docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:1088
msgid "``Wetness index`` [raster]"
msgstr ""

#: ../../docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:1088
msgid "A grid of the wetness index (TWI)."
msgstr ""

#: ../../docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:1090
msgid "**Algorithm ID**: ``taudem:twi``"
msgstr ""

